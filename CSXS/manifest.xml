<?xml version = '1.0' encoding = 'UTF-8'?>
<ExtensionManifest ExtensionBundleId = "ps-autosave.test" ExtensionBundleVersion = "1.0.0" Version = "1.0" xmlns
  <ExtensionList>
    <Extension Id = "ps-autosave.test.panel" Version = "1.0.0" />
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <!--Version 19 is for CEP 8 which supports  CC 2018-->>
      <Host Name = "PHSP" Version "19" />
      <Host Name = "PHXS" Version "19" />
    </HostList>
    <LocaleList>
      <Locale Code = "All" />
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRunTime Name = "CSXS" Version = "7.0" />
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id = "ps-autosave.test">
      <DispatchInfo>
        <Resources>
          <MainPath>./client/index.html</MainPath>
          <ScriptPath>./host/index.jsx</ScriptPath>
          <CEFCommandLine />
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>Text</Menu>
          <Geometry>
            <Size>
              <Height>500</Height>
              <Width>350</Width>
          </Geometry>
          <Icons />
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
